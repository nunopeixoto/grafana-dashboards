          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH first_and_last_dates AS (\n    SELECT \n        MIN(date) AS first_date,\n        MAX(date) AS last_date\n    FROM \n        transactions\n    WHERE \n        category = 'Carro'\n), \ntotal_spent_per_subcategory AS (\n    SELECT \n        subcategory,\n        SUM(debit) AS total_spent\n    FROM \n        transactions\n    WHERE \n        category = 'Carro'\n    GROUP BY \n        subcategory\n),\nmonths_between AS (\n    SELECT \n        TIMESTAMPDIFF(MONTH, first_date, last_date) + 1 AS months\n    FROM \n        first_and_last_dates\n)\nSELECT \n    subcategory,\n    total_spent / months AS average_monthly_spent\nFROM \n    total_spent_per_subcategory, months_between;\n",
          "refId": "A",
