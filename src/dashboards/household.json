          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH monthly_category_sums AS (\n    SELECT \n        category,\n        YEAR(date) AS year,\n        MONTH(date) AS month,\n        SUM(debit) AS total_debit\n    FROM \n        transactions\n    WHERE \n        $__timeFilter(date)\n        AND debit IS NOT NULL\n        AND category <> \"Férias\"\n        AND subcategory <> \"Amortização\"\n    GROUP BY \n        category, YEAR(date), MONTH(date)\n)\nSELECT \n    category,\n    AVG(total_debit) AS value\nFROM \n    monthly_category_sums\nGROUP BY \n    category\nORDER BY \n    value DESC;",
          "refId": "A",
