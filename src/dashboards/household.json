          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH RECURSIVE date_range AS (\n    SELECT \n        DATE_FORMAT(\n            DATE_ADD(\n                DATE_FORMAT($__timeFrom(), '%Y-%m-01'),\n                INTERVAL 0 MONTH\n            ),\n            '%Y-%m-01'\n        ) AS month_date\n    UNION ALL\n    SELECT \n        DATE_ADD(month_date, INTERVAL 1 MONTH)\n    FROM \n        date_range\n    WHERE \n        month_date < DATE_FORMAT($__timeTo(), '%Y-%m-01')\n),\nmonthly_spending AS (\n    SELECT \n        DATE_FORMAT(date, '%Y-%m-01') AS month_date,\n        SUM(debit) AS total_spent\n    FROM \n        transactions\n    WHERE \n        $__timeFilter(date)\n        AND category = 'Carro'\n        AND debit IS NOT NULL\n    GROUP BY \n        DATE_FORMAT(date, '%Y-%m-01')\n)\nSELECT \n    UNIX_TIMESTAMP(dr.month_date) AS time_sec,\n    COALESCE(ms.total_spent, 0) AS total_spent\nFROM \n    date_range dr\nLEFT JOIN \n    monthly_spending ms ON dr.month_date = ms.month_date\nORDER BY \n    dr.month_date;",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
